# Streamlit Cloud Secrets Configuration
# Copy this content to your Streamlit Cloud app's Secrets management
# Settings → Secrets → Paste and Save

# Required: OpenAI API Key
OPENAI_API_KEY = "sk-your-api-key-here"

# Optional: Model selection (defaults to gpt-4o)
OPENAI_MODEL = "gpt-4o"

# Optional: Enable Whisper fallback (not implemented in v1)
ENABLE_WHISPER_FALLBACK = "false"

